{
  "name": "publisher",
  "version": "2.0.0",
  "private": true,
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "rollup:build": "rollup --config rollup.config.mjs",
    "rollup:watch": "rollup --config rollup.config.mjs --watch",
    "clasp:push": "clasp push",
    "clasp:watch": "clasp push --watch",
    "tsc:build": "tsc",
    "tsc:watch": "tsc -w",
    "tw:build": "tailwindcss -i ./src/input.css -o ./src/webpage/static/outputMin.css --minify && node utilities/convertCSS.mjs",
    "tw:watch": "tailwindcss -i ./src/input.css -o ./src/webpage/static/output.css --watch",
    "lint": "eslint . --ext .ts",
    "style": "prettier --write 'src/**/*.{ts,tsx}'",
    "test": "jest",
    "build": "pnpm rollup:build",
    "push": "pnpm clasp:push",
    "watch": "concurrently --kill-others \"pnpm:clasp:watch\" \"pnpm:tw:watch\" \"pnpm:rollup:watch\"",
    "clean": "rimraf dist build"
  },
  "dependencies": {
    "@teal-owl/types": "workspace:^",
    "@teal-owl/watermarking": "workspace:^"
  },
  "devDependencies": {
    "@babel/core": "^7.23.7",
    "@babel/plugin-transform-runtime": "^7.23.7",
    "@babel/preset-env": "^7.23.7",
    "@babel/preset-typescript": "^7.23.3",
    "@google/clasp": "^2.4.2",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@teal-owl/config-prettier": "workspace:^",
    "@types/gapi": "^0.0.47",
    "@types/gapi.auth2": "^0.0.60",
    "@types/google-apps-script": "^1.0.78",
    "@types/google.picker": "^0.0.42",
    "@types/jest": "^29.5.10",
    "@types/node": "^20.10.6",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "babbel": "^3.0.12",
    "concurrently": "^8.2.2",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-googleappsscript": "^1.0.5",
    "eslint-plugin-prettier": "^5.1.2",
    "install": "^0.13.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.0",
    "prettier-plugin-organize-imports": "^3.2.4",
    "prettier-plugin-tailwindcss": "^0.5.10",
    "rimraf": "^5.0.5",
    "rollup": "^4.9.2",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-polyfill-node": "^0.13.0",
    "tailwindcss": "^3.4.0",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
