{
	"name": "publisher",
	"$schema": "../../node_modules/nx/schemas/project-schema.json",
	"targets": {
		"build": {
			"dependsOn": [
				{
					"projects": ["@teal-owl/watermarking"],
					"target": "build"
				}
			],
			"cache": true,
