{
  "name": "teal-owl_reader",
  "version": "0.1.0",
  "description": "My Chrome Extension",
  "private": true,
  "scripts": {
    "watch": "webpack --watch --config config/webpack.dev.js",
    "watch-css": "npx tailwindcss -i ./tailwind.css -o ./src/popup.css --watch",
    "build": "webpack --mode=production --config config/webpack.config.js",
    "format": "prettier --write --ignore-unknown \"{config,public,src}/**/*.{html,css,js,ts,json}\""
  },
  "devDependencies": {
    "copy-webpack-plugin": "^10.2.4",
    "css-loader": "^6.7.3",
    "file-loader": "^6.2.0",
    "mini-css-extract-plugin": "^2.7.2",
    "prettier": "^2.8.3",
    "tailwindcss": "^3.2.4",
    "webpack": "^5.75.0",
    "webpack-cli": "^4.10.0",
    "webpack-merge": "^5.8.0",
	"ts-jest": "^29.0.5",
    "ts-loader": "^9.4.2",
    "typescript": "^4.9.5"
  }
}
