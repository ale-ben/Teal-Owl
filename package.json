{
  "name": "teal-owl_reader",
  "version": "0.1.0",
  "description": "My Chrome Extension",
  "private": true,
  "scripts": {
    "watch": "webpack --watch --config config/webpack.dev.js",
    "watch-css": "npx tailwindcss -i ./tailwind.css -o ./src/popup.css --watch",
    "build": "webpack --mode=production --config config/webpack.config.js",
    "format": "prettier --write --ignore-unknown \"{config,public,src}/**/*.{html,css,js,ts,json}\"",
    "test": "jest"
  },
  "devDependencies": {
    "@types/chrome": "^0.0.211",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.11.19",
    "copy-webpack-plugin": "^10.2.4",
    "css-loader": "^6.7.3",
    "file-loader": "^6.2.0",
    "jest": "^29.4.2",
    "mini-css-extract-plugin": "^2.7.2",
    "prettier": "^2.8.3",
    "tailwindcss": "^3.2.4",
    "ts-jest": "^29.0.5",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5",
    "webpack": "^5.75.0",
    "webpack-cli": "^4.10.0",
    "webpack-merge": "^5.8.0"
  }
}
