{
  "targetDefaults": {
    "lint": {
      "dependsOn": [
        "^lint"
      ],
      "cache": true
    },
    "style": {
      "dependsOn": [
        "^style"
      ],
      "cache": true
    },
    "build": {
      "cache": true,
      "inputs": [
        "production",
        "^production"
      ]
    }
  },
  "affected": {
    "defaultBase": "develop"
  },
  "namedInputs": {
    "default": [
      "{projectRoot}/**/*",
      "sharedGlobals"
    ],
    "sharedGlobals": [],
    "production": [
      "default"
    ]
  }
}
